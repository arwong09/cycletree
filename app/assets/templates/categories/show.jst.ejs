<div class="container" id="container-category-show">
  <div clas="row">
    <div class="col-xs-11 col-xs-offset-1">
      <div class="jumbotron" id="jumbotron-cat-show">
        <h1>Shop Fixed Gear Bicycles</h1>
      </div>
      
      <div class="row">
        <div class="btn-group pull-left">
          <button class="btn btn-options active btn-xs" id="options-all">
            All Cycles
          </button>
      
          <button class="btn btn-options btn-xs" id="options-new" 
            data-condition="New">New Cycles
          </button>
      
          <button class="btn btn-options btn-xs" id="options-used" 
            data-condition="Used" value="Used Cycles">Used Cycles
          </button>
        </div>
    
        <div class="btn-group pull-left">
          <button class="btn btn-default btn-xs dropdown-toggle" 
            type="button" data-toggle="dropdown">
            Price<span class="caret"></span>
          </button>
          <ul class="dropdown-menu">
            <li><a href="#">Any price</a></li>
            <li><a href="#">Under $500</a></li>
            <li><a href="#">$500 - $1000</a></li>
            <li><a href="#">$1000 - $2500</a></li>
            <li><a href="#">Over $2500</a></li>
          </ul>
        </div><!-- PRICE FILTER -->
      </div><!-- FILTERS ROW -->
    </div>
  </div><!-- HEADER ROW -->
  
  <div class="row">
    <div class="col-xs-3">
      <% _(10).times(function(){items1.each(function(item){ %>
        <div class='poster poster-show'>
          <a href="/categories/<%= items1.category_id %>/items/
            <%= item.get('id') %>">
            <img src='<%= item.get('thumb_url') %>' />
          </a>
          <div class="poster-item-title">
            <a href="/categories/<%= items1.category_id %>/items/
              <%= item.get('id') %>">
              <%= item.get('title') %>
            </a>
          </div>
          <div class="poster-item-subtitle">
            <span class="poster-item-owner">
              <a href="/users/<%= item.get('owner_id') %>">
                <%= item.get('owner') %>
              </a>
            </span>
            <span class="poster-item-price">$<%= item.get('price') %></span>
          </div>
        </div>
      <% })}) %>
    </div>
     
